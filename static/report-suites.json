          },
          {
            "$ref": "#/components/parameters/authorization"
          },
          {
            "$ref": "#/components/parameters/x-api-key"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string"
                  },
                  "hitsPerDay": {
                    "type": "integer"
                  },
                  "timezone": {
                    "type": "string",
                    "example": "US/Pacific"
                  },
                  "signUpDate": {
                    "type": "string",
                    "example": "YYYY-MM-DD"
                  },
                  "baseUrl": {
                    "type": "string"
                  },
                  "currencyCode": {
                    "type": "string",
                    "example": "USD"
                  }
                }
              }
            }
          }
        },

        "responses": {
          "201": {
            "description": "Report suite created successfully.",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreatedSuite"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request - Unable to create Report Suite.",
